{
  "name": "mitch-privacy-first",
  "version": "0.0.1",
  "description": "Privacy-first personal data infrastructure - Phase 0",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc",
    "test": "npm run compile && node dist/tests/core.js && node dist/tests/auth.js",
    "evidence": "npm run compile && node dist/tests/evidence.js",
    "evidence:export": "npm run compile && node dist/tests/evidenceExport.js",
    "swarm:test": "npm run compile && node dist/tests/swarmAttack.js",
    "security:deps": "npm audit --audit-level=high",
    "kpi:check": "npm run compile && node dist/tools/kpiCheckCli.js",
    "adjudicate": "npm run compile && node dist/tools/adjudicateCli.js",
    "start": "node dist/server.js",
    "dev": "npm run compile && node dist/server.js"
  },
  "keywords": [
    "privacy",
    "user-control",
    "data-sovereignty"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  }
